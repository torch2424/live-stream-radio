{
  "name": "pi-stream-radio",
  "version": "2.0.0",
  "description": "24/7 music live streaming service",
  "main": "index.js",
  "bin": {
    "live-stream-radio": "./src/index.js"
  },
  "scripts": {
    "precommit": "pretty-quick --staged",
    "start": "node src/index.js live-stream-radio",
    "generate": "rm -rf live-stream-radio && node src/index.js --generate",
    "dev": "node src/index.js live-stream-radio --output dev.flv",
    "prettier": "npm run prettier:fix",
    "lint": " echo \"Listing unlinted files, will show nothing if everything is fine.\" && npx prettier --config .prettierrc --list-different src/**/*.js",
    "lint:fix": "npx prettier --config .prettierrc --write src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torch2424/piStreamRadio.git"
  },
  "author": "Aaron Turner",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/torch2424/piStreamRadio/issues"
  },
  "homepage": "https://github.com/torch2424/piStreamRadio#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "chalkline": "0.0.5",
    "cli-progress": "^2.1.0",
    "fastify": "^1.12.1",
    "find": "^0.2.9",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^7.0.0",
    "imagemin": "^6.0.0",
    "imagemin-gifsicle": "^5.2.0",
    "is-running": "^2.1.0",
    "minimist": "^1.2.0",
    "music-metadata": "^3.1.0",
    "term-img": "^2.1.0"
  },
  "devDependencies": {
    "husky": "^1.0.1",
    "prettier": "1.14.3",
    "pretty-quick": "^1.7.0"
  }
}
